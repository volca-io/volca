    "joi": "^17.9.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-mount": "^4.0.0",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.0.1",
    "snakecase-keys": "^5.4.4",
    "stripe": "^9.12.0",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.0",
    "zxcvbn": "^4.4.2"
  }
