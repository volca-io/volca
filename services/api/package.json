    "copy-webpack-plugin": "^11.0.0",
    "esbuild": "^0.14.51",
    "eslint-plugin-jest": "^26.2.2",
    "jest": "^29.2.2",
    "serverless": "^3.11.0",
    "serverless-domain-manager": "^6.0.4",
    "serverless-esbuild": "^1.32.5",
    "serverless-offline": "^8.8.0",
    "serverless-webpack": "^5.11.0",
    "supertest": "^6.2.2",
