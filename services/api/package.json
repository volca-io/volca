{
  "name": "@volca/api",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "@serverless/typescript": "^3.30.1",
    "@types/aws-lambda": "^8.10.117",
    "@types/correlation-id": "^2.1.2",
    "@types/expect": "^24.3.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa": "^2.13.6",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-mount": "^4.0.2",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "serverless": "^3.32.2",
    "serverless-domain-manager": "^7.0.4",
    "serverless-offline": "^12.0.4",
    "serverless-offline-watcher": "^1.1.0",
    "serverless-webpack": "^5.11.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "wait-on": "^7.0.1",
    "webpack": "^5.86.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "build": "./scripts/build.js",
    "check:lint": "eslint '**/*.{ts,tsx}'",
    "check:types": "tsc --noEmit",
    "dependencies:start": "docker-compose --profile=dev up -d",
    "deploy:staging": "STAGE=staging serverless deploy",
    "deploy:production": "STAGE=production serverless deploy",
    "migrate:up:local": "serverless invoke local -f migrate --data '{ \"type\": \"up\" }'",
    "migrate:down:local": "serverless invoke local -f migrate --data '{ \"type\": \"down\" }'",
    "migrate:latest:local": "serverless invoke local -f migrate --data '{ \"type\": \"latest\" }'",
    "migrate:up:staging": "STAGE=staging serverless invoke -f migrate --data '{ \"type\": \"up\" }'",
    "migrate:down:staging": "STAGE=staging serverless invoke -f migrate --data '{ \"type\": \"down\" }'",
    "migrate:latest:staging": "STAGE=staging serverless invoke -f migrate --data '{ \"type\": \"latest\" }'",
    "migrate:up:production": "STAGE=production serverless invoke -f migrate --data '{ \"type\": \"up\" }'",
    "migrate:down:production": "STAGE=production serverless invoke -f migrate --data '{ \"type\": \"down\" }'",
    "migrate:latest:production": "STAGE=production serverless invoke -f migrate --data '{ \"type\": \"latest\" }'",
    "package": "serverless package",
    "seed": "serverless invoke local -f seed",
    "start": "serverless offline start",
    "start:dev": "yarn dependencies:start && yarn start",
    "test": "./scripts/run-tests.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.359.0",
    "@aws-sdk/client-sesv2": "^3.379.1",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@sentry/node": "^7.56.0",
    "aws-jwt-verify": "^4.0.0",
    "aws-lambda": "^1.0.7",
    "correlation-id": "^5.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.0",
    "koa-mount": "^4.0.0",
    "objection": "^3.0.1",
    "pg": "8.10.0",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.2.0",
    "stripe": "^12.9.0",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0",
    "winston": "^3.9.0"
  }
}
