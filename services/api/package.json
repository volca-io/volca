    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-mount": "^4.0.1",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^8.0.11",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/zxcvbn": "^4.4.1",
    "chai": "^4.3.6",
